<div class="main-content">
    <div class="breadcrumbs-container">
        <ul class="breadcrumb">
                    <li>Dashboard</li>
        </ul>
    </div>
    
    <div class="row gx-3 gy-3 mb-3 row1-data"> 
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/bidding-session" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <img src="/image/admin/dashboard/biddingSessions.png" alt="bidding-sessions">
                      <div>
                        <h5 class="fw-normal pt-1">BIDDING SESSIONS</h5>
                      </div>
                    </div>
                </div>
            </a>
        </div>
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/program" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <div>
                            <img src="/image/admin/dashboard/programs.png" alt="programs">
                        </div>
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">PROGRAMS</h5>
                            <h3 class="align-self-end"><%= locals.programCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/course" class="tab-expand">
                <div class="card row1-data-card" >
                    <div class="card-body d-flex justify-content-between">
                        <div>
                            <img src="/image/admin/dashboard/courses.png" alt="">
                        </div>
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">COURSES</h5>
                            <h3 class="align-self-end"><%= locals.courseCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/program-session" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <img src="/image/admin/dashboard/room-type.PNG" alt="program-sessions">
                      <div class="d-flex flex-column">
                        <h5 class="fw-normal pt-1">PROGRAM SESSIONS</h5>
                        <h3 class="align-self-end"><%= locals.programSessionCount %></h3>
                      </div>
                    </div>
                </div>
            </a>
        </div>
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/areas" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <img src="/image/admin/dashboard/area.jpg" alt="areas">
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">AREAS</h5>
                            <h3 class="align-self-end"><%= locals.areaCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/specialization" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <div>
                            <img src="/image/admin/dashboard/concentration.png" alt="specialization">
                        </div>
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">SPECIALIZATIONS</h5>
                            <h3 class="align-self-end"><%= locals.concentrationCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>

    


        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/student" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <div>
                            <img src="/image/admin/dashboard/student.png" alt="">
                        </div>
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">STUDENTS</h5>
                            <h3 class="align-self-end"><%= locals.studentCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    
       
        <div class="col-xl-3 col-lg-4 col-md-12">
            <a href="/admin/division-batches" class="tab-expand">
                <div class="card row1-data-card">
                    <div class="card-body d-flex justify-content-between">
                        <div>
                            <img src="/image/admin/dashboard/division.png" alt="">
                        </div>
                        <div class="d-flex flex-column">
                            <h5 class="fw-normal pt-1">DIVISION BATCHES</h5>
                            <h3 class="align-self-end"><%= locals.divisionBatchCount %></h3>
                        </div>
                    </div>
                </div>
            </a>
        </div>

        <div class="modal" id="createBiddingSession" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-fullscreen">
                <div class="modal-content">
                    <div class="modal-header">
                        <h3>Bidding Session</h3>
                        <button class="btn-close" data-bs-dismiss="modal"></button>
                    </div>
                    <div class="modal-body">
    
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-success">Create Bidding Session</button>
                    </div>
                </div>
            </div> 
        </div>  
</div>
</main>

<%- include('../partials/footer') %>

